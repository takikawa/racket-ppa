	@RUN_RACKET_CGC@ $(SELF_RACKET_FLAGS) -cqu $(srcdir)/check-sdep.rkt

$(XSRCDIR)/precomp.h : $(XFORMDEP_NOPRE)  $(srcdir)/../src/schvers.h
	env XFORM_PRECOMP=yes $(XFORM_NOPRECOMP) $(XSRCDIR)/precomp.h $(srcdir)/precomp.c

