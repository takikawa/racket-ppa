all: libwxscheme.@LIBSFX@

libwxscheme.@LIBSFX@ : $(OBJECTS) 
	$(AR) $(ARFLAGS) libwxscheme.@LIBSFX@ $(OBJECTS)
	$(RANLIB) libwxscheme.@LIBSFX@

test-file:
	rm -f $(srcdir)/../../../tests/mred/tests.ss
	$(MAKE) $(srcdir)/../../../tests/mred/tests.ss

kernel.ss : $(srcdir)/../../../collects/mred/private/kernel.ss 

$(srcdir)/../../../collects/mred/private/kernel.ss : $(SSDESCS) $(srcdir)/khead.ss $(srcdir)/ktail.ss 
	cat $(srcdir)/khead.ss $(SSDESCS) $(srcdir)/ktail.ss > $(srcdir)/../../../collects/mred/private/kernel.ss

ss: $(SSDESCS)

ccsources: $(SOURCES)

clean:
	rm -f *.@LTO@ *.@LIBSFX@ *.d

repatriate:
	mv $(HIDEDIR)/wxs_objects/objects .
	rmdir $(HIDEDIR)/wxs_objects

